<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="raybiztech master suite" thread-count="4"

	parallel="tests" verbose="4">

    <listeners>
		
		<listener class-name="com.qa.raybiztech.listeners.ExtentReportListener" />
		
		<listener class-name="com.qa.raybiztech.listeners.AnnotationTransformer" />

	</listeners>
	
	<test name="Homepage validation">
		
		<classes>
			<class name="com.qa.raybiztech.tests.HomePageTest" />
		</classes>
	</test>
	
	
	<test name="ArtificialIntelligence validation">
		<classes>
			<class name="com.qa.raybiztech.tests.ArtificialIntelligencePageTest" />
		</classes>
	</test>
	
	<test name="Contact validation">
		<classes>
			<class name="com.qa.raybiztech.tests.ContactPageTest" />
		</classes>
	</test>
	
</suite>
